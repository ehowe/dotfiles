[user]
  name = Eugene Howe
  email = eugene@xtreme-computers.net
[color]
  diff = auto
  status = auto
  branch = auto
[alias]
  a = add
  ap = add -p
  br = branch
  ci = commit
  co = checkout
  cp = cherry-pick
  ct = checkout -t
  d = diff
  dc = diff --cached
  fa = fetch --all
  l = log --decorate --oneline --graph
  ll = log
  ls = branch
  mf = merge --ff-only
  ms = merge --squash
  pl = pull --rebase
  ra = rebase --abort
  rc = rebase --continue
  recent = "for-each-ref --format='%(committerdate:relative) %(refname:short)' --sort=-committerdate refs/heads/ --count 25"
  ri = rebase -i
  ro = !git fetch origin && git reset --hard origin/master
  s = status -s
  sh = !git-sh
  st = status
  v = show
[core]
  editor = /usr/bin/vim
  excludesfile = ~/.gitignore
  whitespace = trailing-space,space-before-tab,blank-at-eol
[apply]
  whitespace = fix
[push]
  default = current
[branch]
  autosetuprebase = always
[github]
  user = eugene@xtreme-computers.net
  ;password = !security find-generic-password -gs github.password 2>&1 | grep "^password" | awk '{print $2}'
  password = !security 2>&1 >/dev/null find-generic-password -gs github.password | ruby -e 'print $1 if STDIN.gets =~ /^password: \\\"(.*)\\\"$/'
[rerere]
  enabled = true
